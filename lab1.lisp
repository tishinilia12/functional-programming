(defun TBS (x1 y1 x2 y2) (equal(abs(- x1 x2))(abs(- y1 y2))))
(defun SBS (x1 y1 x2 y2 x3 y3)
	(if (and(TBS x1 y1 x3 y3)(TBS x2 y2 x3 y3)) 
		(values x3 y3)
		(if (equal x3 8) 
			(if (equal y3 8)
				(values NIL)
			 	(SBS x1 y1 x2 y2 1 (+ y3 1)))
	 		(SBS x1 y1 x2 y2 (+ x3 1)  y3))))
(defun bishop-moves (x1 y1 x2 y2) (if (TBS x1 y1 x2 y2) (values T) (SBS x1 y1 x2 y2 1 1)))